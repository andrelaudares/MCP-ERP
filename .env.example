# Exemplo de variáveis de ambiente para o MCP ERP App

# Chaves de API Omie
OMIE_APP_KEY="sua_chave_de_aplicacao"
OMIE_APP_SECRET="sua_chave_de_aplicacao_secreta"

# Chave de API Google Gemini (IA)
GOOGLE_API_KEY="sua_chave_de_api_google"

# Chave de API Google Gemini (usada pelo agente PydanticAI)
GEMINI_API_KEY="sua_chave_google_gemini"

# Variáveis do banco de dados principal
POSTGRES_USER=myuser
POSTGRES_PASSWORD=mypassword
POSTGRES_DB=mydatabase

# Variáveis para Evolution API e seus serviços
EVOLUTION_API_KEY=change-me
EVOLUTION_DB_NAME=evolution_db
EVOLUTION_DB_USER=evolution_user
EVOLUTION_DB_PASSWORD=senha_forte
EVOLUTION_DB_HOST_PORT=5433
EVOLUTION_DB_CONNECTION_URI=postgresql://evolution_user:senha_forte@evolution_postgres:5432/evolution_db

# Redis
CACHE_REDIS_ENABLED=true
CACHE_REDIS_URI=redis://redis:6379/1
CACHE_REDIS_PREFIX_KEY=evolution_v2

# RabbitMQ
RABBITMQ_DEFAULT_USER=user
RABBITMQ_DEFAULT_PASS=password
RABBITMQ_ENABLED=true
RABBITMQ_URI=amqp://user:password@rabbitmq:5672

# Minio/S3
MINIO_ROOT_USER=minio_admin
MINIO_ROOT_PASSWORD=minio_password
S3_ENABLED=false
S3_ACCESS_KEY=minio_admin
S3_SECRET_KEY=minio_password
S3_BUCKET=evolution
S3_ENDPOINT=http://minio:9000
S3_PORT=9000
S3_USE_SSL=false

# Audio Converter
AUDIO_CONVERTER_API_KEY=changeme
AUDIO_CONVERTER_OPENAI_API_KEY=
AUDIO_CONVERTER_GROQ_API_KEY=
API_AUDIO_CONVERTER=http://audio-converter:4040/process-audio
ENABLE_TRANSCRIPTION=true
TRANSCRIPTION_PROVIDER=openai
TRANSCRIPTION_LANGUAGE=pt

# App/Agente
AGENT_PROVIDER=

# Variável para a chave de API do OpenAI
OPENAI_API_KEY=

# Variável para o modelo do OpenAI
OPENAI_MODEL=